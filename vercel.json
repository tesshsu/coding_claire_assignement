{
  "version": 2,
  "builds": [
    {
      "src": "back/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "back/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/front/index.html"
    }
  ],
  "staticFiles": {
    "staticPath": "front"
  }
}
